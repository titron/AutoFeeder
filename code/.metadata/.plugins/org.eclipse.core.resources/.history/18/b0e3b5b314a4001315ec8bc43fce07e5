	0x3EU,0x6EU,0x22U,0x2FU,0x41U,0x0CU,0x41U,0x0CU,0x7FU,0xFCU,0x7FU,0xF8U,0x00U,0x00U,0x00U,0x00U,/*0xb2*/
};


U8 DrawBuf[72];

/*****************************************************************************
* ID			 : 1.0
* Outline		 : code to address
* Include		 : none
* Declaration	 : static void FontcodeToFlashAddress(U16 Code,U32 *pFlashAddress, U8 *pFontWidth)
* Description	 : get address 
* Argument 		 : Code, the code of unicode 
*                : pFlashAddress, pointer to return address
*              	 : pFontWidth,pointer to return font width
* Return Value 	 : void
* Calling Functions : none
*****************************************************************************/
static void FontcodeToFlashAddress(U16 Code,U32 *pFlashAddress, U8 *pFontWidth,U8 font)
{
	U32 GBoffent;
	U16 Code_c = Code;
	switch(font)
	{
	case FONT16:
		break;
	case FONT24:
			GBoffent = ((Code_c>>8U)-0xb0U)*0x5eU;
			GBoffent +=((Code_c&0x00ffU)-0xa1U);
			
			*pFlashAddress = (GBoffent*FONT24_BYTE)+FLASH_ADDRESS_GB2312_24;
			*pFontWidth = FONT24_FULL_WIDTH;
	    break;
	default:
	    break;
	}

}

/*****************************************************************************
* ID			 : 1.0
* Outline		 : Get font dot
* Include		 : none
* Declaration	 : static void GetFlashFontToBuf(U16 code,U16 * drawbuf,U8 * FontWidth)
* Description	 : Get font dot
* Argument 		 : Code, the code of GB 
*                : drawbuf, pointer to buf
*              	 : FontWidth, Full width or Half width
* Return Value 	 : void
* Calling Functions : static void FontcodeToFlashAddress(U16 Code,U32 *pFlashAddress, U8 *pFontWidth)
*****************************************************************************/

static void GetFlashFontToBuf(U16 code,U8 * drawbuf,U8 * FontWidth,U8 font)
{
	U32 FlashAddress;

	FontcodeToFlashAddress(code,&FlashAddress,FontWidth,font);

	if (drawbuf==0)
	{
		return;
	}else
	{
		if(font == FONT16)
		{
				If_Flash_Read32BytesData(FlashAddress,drawbuf);

		}else if (font == FONT24)
		{
				If_Flash_ReadData(FlashAddress,FONT24_BYTE_NUM_FULL,drawbuf);
		}else
		{
			/*Nothing to do*/
		}
	}

	return;


}






/*****************************************************************************
* ID				 : 1.0
* Outline		 : LCD draw text code
* Include		 : none
* Declaration		 : void LCD_TextA_16(U8 *code_buf,U16 PosX,U16 PosY,U8 space)
* Description		 : LCD module
* Argument 		 : code_buf,PosX,PosY,space
* Return Value 	 : void
* Calling Functions : none
*****************************************************************************/
void LCD_TextA_16(U8 *code_buf,U16 PosX,U16 PosY,U8 space)
{
	U8 drawtemp;
	static U8 oldBuf[16];
	U8 OledBuf[8];
	U8 lcdHorizontal,lcdOffend;
	U8 u8_i,u8_j;
	U8 FontWidth;
	U8 offset,drawOffset;
	U16 Codelong;
	U8 const *Fontstore;

	if(code_buf==NULL)
	{
		return;
	}

	offset = (U8)(PosX%4U);

	while (*code_buf)
	{
		Codelong = *code_buf;
		if(Codelong<0x80U)
		{
			/* modify 20110631  the date type of (*code_buf) is U8,there may be out of range.
			memcpy(DrawBuf,CodeASCII+(*code_buf-ASCII_START_CODE)*FONT16_BYTE_NUM_HALF,FONT16_BYTE_NUM_HALF);
			*/
			Fontstore = CodeASCII+(Codelong-ASCII_START_CODE)*FONT16_BYTE_NUM_HALF;
			memcpy(DrawBuf,Fontstore,FONT16_BYTE_NUM_HALF);
			FontWidth = FONT16_HALF_WIDTH;
		}else if((Codelong>0x7fU)&&(Codelong<0xffU))
		{
			Fontstore = CodeIco+(Codelong-DEFINEICO_START_CODE)*FONT16_BYTE_NUM_FULL;
			memcpy(DrawBuf,Fontstore,FONT16_BYTE_NUM_FULL);
			FontWidth = FONT16_FULL_WIDTH;
		}else
		{
			memcpy(DrawBuf,CodeASCII,FONT16_BYTE_NUM_HALF);
			FontWidth = FONT16_HALF_WIDTH;
		}

		if(offset)
		{
			if(FontWidth==FONT16)
			{			
				for (u8_i=0U;u8_i<FONT16;u8_i++)
				{
					drawOffset =u8_i<<1U;
					drawtemp = DrawBuf[drawOffset];
					DrawBuf[drawOffset]=(drawtemp>>offset)|(oldBuf[u8_i]<<(8U-offset));
					oldBuf[u8_i] = DrawBuf[drawOffset+1U];
					DrawBuf[drawOffset+1U]=(DrawBuf[drawOffset+1U]>>offset)|(drawtemp<<(8U-offset));


				}
			}else if (FontWidth == FONT16/2U)
			{
				for (u8_i=0U;u8_i<FONT16;u8_i++)
				{
					drawtemp = DrawBuf[u8_i];
					DrawBuf[u8_i]=(drawtemp>>offset)|(oldBuf[u8_i]<<(8U-offset));
					oldBuf[u8_i] = drawtemp;
				}
			}
		}

		Drv_OLED_GraphicWrite(PosX,PosY,FontWidth,FONT16,DrawBuf,FONT16*(FontWidth/8U));

		PosX+=FontWidth;
		/*length limit*/
		if (PosX>OLED_MAXLINE+32U)
		{
			break;
		}else
		{
			/*do nothing*/
		}

		code_buf++;

	}
	
}

/*****************************************************************************
* ID				 : 1.0
* Outline		 : LCD draw text code
* Include		 : none
* Declaration		 : void LCD_TextA_24(U8 *code_buf,U16 PosX,U16 PosY,U8 space)
* Description		 : LCD module
* Argument 		 : code_buf,PosX,PosY,space
* Return Value 	 : void
* Calling Functions : none
*****************************************************************************/
void LCD_TextA_24( U8 *code_buf,U16 PosX,U16 PosY,U8 space)
{
	U8 drawtemp,drawtemp1;
	static U8 oldBuf[24];
	U8 lcdHorizontal,lcdOffend;
	U8 u8_i,u8_j;
	U8 FontWidth;
	U8 offset,drawOffset;
	U16 Codelong;
	U8 const * Codestore;

	if(code_buf==NULL)
	{
		return;
	}
	
	offset =(U8)(PosX%4U);

	memset(oldBuf,0U,sizeof(oldBuf));

	while (*code_buf)
	{
		memset(DrawBuf,0U,sizeof(DrawBuf));
		/*ansi code or GB code*/
		if (*code_buf<0x80U)
		{
			Codelong = *code_buf;
			Codestore = CodeASCII24+(Codelong-ASCII_START_CODE)*FONT24_BYTE_NUM_HALF;
			memcpy(DrawBuf,Codestore,FONT24_BYTE_NUM_HALF);
			FontWidth = FONT24_HALF_WIDTH;
			code_buf++;
		}else if (((code_buf[1]>=0xA1U)&&(code_buf[1]<=0xfeU))&&((code_buf[0]>=0xb0U)&&(code_buf[0]<=0xf7U)))
		{
			Codelong = code_buf[0];
			Codelong = Codelong<<8U;
			Codelong |= (code_buf[1]);
			GetFlashFontToBuf(Codelong,DrawBuf,&FontWidth,FONT24);
			FontWidth = FONT24_FULL_WIDTH;
			code_buf+=2U;
		}
		else	
		{
			/*speac code*/
			Codelong = 0x20U;
			Codestore = CodeASCII24+(Codelong-ASCII_START_CODE)*FONT24_BYTE_NUM_HALF;
			memcpy(DrawBuf,Codestore,FONT24_BYTE_NUM_HALF);
			FontWidth = FONT24_HALF_WIDTH;
			code_buf++;
		}


		/* Offset shift*/
		if(offset)
		{
			if(FontWidth==FONT24_FULL_WIDTH)
			{			
				for (u8_i=0U;u8_i<FONT24;u8_i++)
				{
					drawOffset =u8_i*3U;
					drawtemp = DrawBuf[drawOffset];
					drawtemp1 = DrawBuf[drawOffset+1U];
					DrawBuf[drawOffset]=(drawtemp>>offset)|(oldBuf[u8_i]<<(8U-offset));
					/*oldBuf[u8_i] = DrawBuf[drawOffset+1];*/
					DrawBuf[drawOffset+1U] =(drawtemp1>>offset)|(drawtemp<<(8U-offset));
					oldBuf[u8_i] = DrawBuf[drawOffset+2U];
					DrawBuf[drawOffset+2U] =(DrawBuf[drawOffset+2U]>>offset)|(drawtemp1<<(8U-offset)); 
				}
			}else if (FontWidth == FONT24_HALF_WIDTH)
			{
				for (u8_i=0U;u8_i<FONT24;u8_i++)
				{
					drawOffset =u8_i<<1U;
					drawtemp = DrawBuf[drawOffset];
					DrawBuf[drawOffset]=(drawtemp>>offset)|(oldBuf[u8_i]<<(8U-offset));
					oldBuf[u8_i] = DrawBuf[drawOffset+1U]>>4U;
					DrawBuf[drawOffset+1U]=(DrawBuf[drawOffset+1U]>>offset)|(drawtemp<<(8U-offset));
				}
			}
		}

		if (FontWidth == FONT24_FULL_WIDTH)
		{
			Drv_OLED_GraphicWrite(PosX,PosY,FONT24_FULL_WIDTH,FONT24,DrawBuf,FONT24_BYTE_NUM_FULL);
		} 
		else
		{
			Drv_OLED_GraphicWrite(PosX,PosY,(U16)16,FONT24,DrawBuf,FONT24_BYTE_NUM_HALF);
		}

		PosX+=FontWidth;

		/*length limit*/
		if (PosX>OLED_MAXLINE+32U)
		{
			break;
		}else
		{
			/*do nothing*/
		}

	}

}

#if 1
void LCD_TextA_Rev(U8 *code_buf,U16 PosX,U16 PosY,U8 font,U8 mode)
{
	U8 drawtemp;
	static U8 oldBuf[16];
	U8 OledBuf[8];
	U8 lcdHorizontal,lcdOffend;
	U8 u8_i,u8_j;
	U8 FontWidth;
	U8 offset,drawOffset;
	U16 Codelong;
	U8 const * Codestore;

	if(code_buf==NULL)
		return;


	offset = PosX%4U;

	while (*code_buf)
	{
		Codelong = *code_buf;
		//GetFlashFontToBuf(CodeUnicode,DrawBuf,&FontWidth,FONT16);
		if(*code_buf<0x80U)
		{
			Codelong = *code_buf;
			Codestore = CodeASCII+(Codelong-ASCII_START_CODE)*FONT16_BYTE_NUM_HALF;
			memcpy(DrawBuf,Codestore,FONT16_BYTE_NUM_HALF);
			FontWidth = FONT16_HALF_WIDTH;
		}else if((*code_buf>0x7fU)&&(*code_buf<0xffU))
		{
			Codelong = *code_buf;
			Codestore = CodeIco+(Codelong-DEFINEICO_START_CODE)*FONT16_BYTE_NUM_FULL;
			memcpy(DrawBuf,Codestore,FONT16_BYTE_NUM_FULL);
			FontWidth = FONT16_FULL_WIDTH;
		}else
		{
			memcpy(DrawBuf,CodeASCII,FONT16_BYTE_NUM_HALF);
			FontWidth = FONT16_HALF_WIDTH;
		}

		if(offset)
		{
			if(FontWidth==FONT16)
			{			
				for (u8_i=0U;u8_i<FONT16;u8_i++)
				{
					drawOffset =u8_i<<1U;
					drawtemp = DrawBuf[drawOffset];
					DrawBuf[drawOffset]=(drawtemp>>offset)|(oldBuf[u8_i]<<(8U-offset));
					oldBuf[u8_i] = DrawBuf[drawOffset+1U];
					DrawBuf[drawOffset+1U]=(DrawBuf[drawOffset+1U]>>offset)|(drawtemp<<(8U-offset));


				}
			}else if (FontWidth = FONT16/2U)
			{
				for (u8_i=0U;u8_i<FONT16;u8_i++)
				{
					drawtemp = DrawBuf[u8_i];
					DrawBuf[u8_i]=(drawtemp>>offset)|(oldBuf[u8_i]<<(8U-offset));
					oldBuf[u8_i] = drawtemp;
				}
			}
		}

		for (u8_i = 0U;u8_i<32U;u8_i++)
		{
			DrawBuf[u8_i]=~DrawBuf[u8_i];
		}

		Drv_OLED_GraphicWrite(PosX,PosY,FontWidth,FONT16,DrawBuf,FONT16*(FontWidth/8U));

		//Drv_OLED_GraphicRead(PosX,PosY,FontWidth,FONT16,drawbuf,32);
		PosX+=FontWidth;
		/*length limit*/
		if (PosX>OLED_MAXLINE+32U)
		{
			break;
		}else
		{
			/*do nothing*/
		}		

		code_buf++;

	}
}
#endif


/*****************************************************************************
* ID				 : 1.0
* Outline		 : LCD draw text code
* Include		 : none
* Declaration		 : void LCD_TextA(U8 *code_buf,U16 PosX,U16 PosY,U8 font)
* Description		 : LCD module
* Argument 		 : code_buf,PosX,PosY,font
* Return Value 	 : void
* Calling Functions : none
*****************************************************************************/
void LCD_TextA( U8 *code_buf,U16 PosX,U16 PosY,U8 font)
{
	switch(font)
	{
	case FONT16:
		LCD_TextA_16(code_buf,PosX,PosY,0U);
		break;
	case FONT24:
		LCD_TextA_24(code_buf,PosX,PosY,0U);
		break;
	default:
		break;
	}
}

/*****************************************************************************
* ID				 : 1.0
* Outline			 : LCD draw text code
* Include			 : none
* Declaration		 : void LCD_Draw_TextRoll24(U8 *code_buf,U16 PosX,U16 PosY,U16 StrWidth)
* Description		 : LCD module
* Argument 			 : code_buf,PosX,PosY,StrWidth
* Return Value 		 : void
* Calling Functions	 : none
*****************************************************************************/
void LCD_Draw_TextRoll24(U8 *CodeBuf,S16 PosX,U16 PosY,U16 StrWidth)
{
	U8 drawtemp,drawtemp1;
	U8 oldBuf[24];
	U8 lcdHorizontal,lcdOffend;
	U8 u8_i,u8_j;
	U8 FontWidth= FONT24_HALF_WIDTH;
	U8 offset,drawOffset;
	U16 Codelong;
	U8 const * Codestore;

	if(CodeBuf==NULL)
		return;

	if(PosX>(S16)OLED_MAXLINE+FONT24)
		return;

	memset(oldBuf,0U,sizeof(oldBuf));

	while (*CodeBuf)
	{
		memset(DrawBuf,0U,sizeof(DrawBuf));
		/*ansi code or GB code*/
		if (*CodeBuf<0x80U)
		{
			Codelong = *CodeBuf;
			Codestore = CodeASCII24+(Codelong-ASCII_START_CODE)*FONT24_BYTE_NUM_HALF;
			memcpy(DrawBuf,Codestore,FONT24_BYTE_NUM_HALF);
			FontWidth = FONT24_HALF_WIDTH;
			CodeBuf++;
		}else if (((CodeBuf[1]>=0xA1U)&&(CodeBuf[1]<=0xfeU))&&((CodeBuf[0]>=0xb0U)&&(CodeBuf[0]<=0xf7U)))
		{
			Codelong = CodeBuf[0];
			Codelong = Codelong<<8U;
			Codelong |= (CodeBuf[1]);
			GetFlashFontToBuf(Codelong,DrawBuf,&FontWidth,FONT24);
			FontWidth = FONT24_FULL_WIDTH;
			CodeBuf+=2U;
		}
		else	
		{
			Codelong = 0x20U;
			Codestore = CodeASCII24+(Codelong-ASCII_START_CODE)*FONT24_BYTE_NUM_HALF;
			memcpy(DrawBuf,Codestore,FONT24_BYTE_NUM_HALF);
			FontWidth = FONT24_HALF_WIDTH;
			CodeBuf++;
		}

		if (PosX>=(S16)OLED_MAXLINE+FONT24)
		{
			PosX = PosX-OLED_MAXLINE-FONT24-StrWidth;
		}
		if (PosX<0)
		{
			PosX +=FontWidth;
			memset(oldBuf,0U,sizeof(oldBuf));
			continue;
		}
		offset = PosX%4U;

		/* Offset shift*/
		if(offset)
		{
			if(FontWidth==FONT24_FULL_WIDTH)
			{			
				for (u8_i=0U;u8_i<FONT24;u8_i++)
				{
					drawOffset =u8_i*3U;
					drawtemp = DrawBuf[drawOffset];
					drawtemp1 = DrawBuf[drawOffset+1];
					DrawBuf[drawOffset]=(drawtemp>>offset)|(oldBuf[u8_i]<<(8U-offset));
					//oldBuf[u8_i] = DrawBuf[drawOffset+1];
					DrawBuf[drawOffset+1U] =(drawtemp1>>offset)|(drawtemp<<(8U-offset));
					oldBuf[u8_i] = DrawBuf[drawOffset+2];
					DrawBuf[drawOffset+2] =(DrawBuf[drawOffset+2]>>offset)|(drawtemp1<<(8U-offset)); 
				}
			}else if (FontWidth == FONT24_HALF_WIDTH)
			{
				for (u8_i=0U;u8_i<FONT24;u8_i++)
				{
					drawOffset =u8_i<<1U;
					drawtemp = DrawBuf[drawOffset];
					DrawBuf[drawOffset]=(drawtemp>>offset)|(oldBuf[u8_i]<<(8U-offset));
					oldBuf[u8_i] = DrawBuf[drawOffset+1]>>4U;
					DrawBuf[drawOffset+1]=(DrawBuf[drawOffset+1]>>offset)|(drawtemp<<(8U-offset));
				}
			}
		}

		if (FontWidth == FONT24_FULL_WIDTH)
		{
			Drv_OLED_GraphicWrite_Offset(PosX,PosY,FONT24_FULL_WIDTH,FONT24,DrawBuf,FONT24_BYTE_NUM_FULL,0x16);
		} 
		else
		{
			Drv_OLED_GraphicWrite_Offset(PosX,PosY,16U,FONT24,DrawBuf,FONT24_BYTE_NUM_HALF,0x16);
		}

		//Drv_OLED_GraphicRead(PosX,PosY,FontWidth,FONT16,DrawBuf,32);
		PosX+=FontWidth;

	}

	for (u8_i = 0U;u8_i<FONT24;u8_i++)
	{
		oldBuf[u8_i]=oldBuf[u8_i]<<(8U-offset);
	}
	Drv_OLED_GraphicWrite_Offset(PosX,PosY,8U,FONT24,oldBuf,24U,0x16U);



}





/*****************************************************************************
* ID				 : 1.0
* Outline			 : DrawTextWidth
* Include			 : none
* Declaration		 : U16 DrawTextWidthW(U16 *Code,U8 font)
* Description		 : Display module
* Argument 			 : Code ,the point of code
* Return Value 		 : the width of display 
* Calling Functions : none
*****************************************************************************/
U16 GetDrawTextWidthA(U8 *CodeBuf,U8 font)
{
	U16 TotleWidth;
	U8  FontWidth;
	u16 Temp,CodeUnicode;
	U8	FontHalfWidth,FontFullWidth;

	TotleWidth = (U16)0;

	switch(font)
	{
	case FONT16:
		FontHalfWidth = FONT16_HALF_WIDTH;
		FontFullWidth = FONT16_FULL_WIDTH;
		break;
	case FONT24:
		FontHalfWidth = FONT24_HALF_WIDTH;
		FontFullWidth = FONT24_FULL_WIDTH;
	    break;
	default:
		FontHalfWidth = FONT16_HALF_WIDTH;
		FontFullWidth = FONT16_FULL_WIDTH;
	    break;
	}

	while (*CodeBuf)
	{

		/*ansi code or GB code*/
		if (*CodeBuf<0x80)
		{
			FontWidth = FontHalfWidth;
			CodeBuf++;
		}else if (((CodeBuf[1]>=0xA1)&&(CodeBuf[1]<=0xfe))&&((CodeBuf[0]>=0xb0)&&(CodeBuf[0]<=0xf7)))
		{
			FontWidth = FontFullWidth;
			CodeBuf+=2;
		}
		else	
		{
			FontWidth = FontHalfWidth;
			CodeBuf++;
		}
		
		TotleWidth+=FontWidth;

	}

	return TotleWidth;
}
/*****************************************************************************
* ID				 : 1.0
* Outline			 : DrawTextWidth
* Include			 : none
* Declaration		 : U16 ConvertUTF8ToGBCode(U8 *utf8code,U16 utf8len,U8 *GBcode,U16 GBcodelen)
* Description		 : Display module
* Argument 			 : 
* Return Value 		 : void 
* Calling Functions : none
*****************************************************************************/
void ConvertUTF8ToGBCode(U8 *utf8code,U16 utf8len,U8 *GBcode,U16 GBcodelen)
{
	U16 covnumutf,covnumgb;
	U16 Temp;
	U32 TableAdr;

	covnumutf = 0;
	covnumgb = 0;

	for(covnumutf=0;covnumutf<utf8len;)
	{
		/* Check utf head*/
		if ((*utf8code&0x80)==00)/*0x0xxxxxxx U00000000-U0000007F ASCII*/
		{
			*GBcode = *utf8code;
			GBcode++;
			utf8code++;
			covnumutf++;
			covnumgb++;
		}else if ((*utf8code&0xF0)==0xE0)/*0x1110xxxx U00000800-U0000FFFF BMP UNICODE*/
		{
			Temp = (*utf8code&0x0F)<<4;
			Temp |= ((utf8code[1]>>2)&0x0F);
			Temp = Temp<<8;

			Temp |= ((utf8code[1]<<6)&0xC0);
			Temp |=(utf8code[2]&0x3F);

			if((Temp>=UNICODE_CJK_MIN)&&(Temp<=UNICODE_CJK_MAX))
			{
				TableAdr =FLASH_ADDRESS_UNICODETOGB2312_TABLE+(Temp-UNICODE_CJK_MIN)*2;
				If_Flash_ReadData(TableAdr,2,GBcode);

				GBcode+=2;
				utf8code+=3;
				covnumutf+=3;
				covnumgb+=2;
			}else
			{
				*GBcode = 0x20;
				GBcode++;
				utf8code++;
				covnumutf++;
				covnumgb++;
			}
		}else if ((*utf8code&0xE0)==0xC0)/*0x110xxxxx U0000080-U000007FF*/
		{
			utf8code+=2;
			GBcode[0] = 0x20;
			GBcode++;
			covnumutf+=2;
			covnumgb++;
		}else if ((*utf8code&0xC0)==0x80)/**/
		{
			utf8code++;
			covnumutf++;
		}else if ((*utf8code&0xF8)==0xF0)/*0x11110xxx U00010000-U001FFFF*/
		{
			GBcode[0] = 0x20;
			GBcode++;
			utf8code+=4;
			covnumutf+=4;
			covnumgb++;
		}else if ((*utf8code&0xFC)==0xF8)/*0x111110xx U00200000-U03FFFFFF*/
		{
			GBcode[0] = 0x20;
			GBcode++;
			utf8code+=5;
			covnumutf+=5;
			covnumgb++;
		}else if ((*utf8code&0xFE)==0xFC)/*0x1111110x U04000000-U7FFFFFFF*/
		{
			GBcode[0] = 0x20;
			GBcode++;
			utf8code+=6;
			covnumutf+=6;
			covnumgb++;
		}else
		{
			utf8code++;
			covnumutf++;
		}

		/*Unicode buf protect*/
		if(covnumgb>GBcodelen-2)
			break;
	}

	/*add null cell*/
	GBcode[0] = 0x00; 

	/*unicode to GBcode*/



}
/*****************************************************************************
* ID				 : 1.0
* Outline			 : ConvertUnicodeToGBCode
* Include			 : none
* Declaration		 : U16 ConvertUnicodeToGBCode(U8 *utf8code,U16 utf8len,U8 *GBcode,U16 GBcodelen)
* Description		 : Display module
* Argument 			 : 
* Return Value 		 : void 
* Calling Functions : none
*****************************************************************************/
void ConvertUnicodeToGBCode(U8 *code,U16 len,U8 flag)
{
	U16 Temp;
	U32 TableAdr;
	U8 *pcode;
	U16 *pU16code;
	U16 clen;

	pcode = code;
	Temp = 0xffff;
	clen = 0;

	for (clen=0;clen<len;clen+=(U16)2)
	{
		if (flag == LITTLE_ENDIAN)
		{
			Temp = code[1];
			Temp = Temp<<(U16)8;
			Temp |=code[0];
		}else
		{
			Temp = code[0];
			Temp = Temp<<(U16)8;
			Temp |=code[1];
		}

		if ((Temp>=UNICODE_CJK_MIN)&&(Temp<=UNICODE_CJK_MAX))
		{
			TableAdr =FLASH_ADDRESS_UNICODETOGB2312_TABLE+(Temp-UNICODE_CJK_MIN)*2;
			If_Flash_ReadData(TableAdr,(U16)2,pcode);
			pcode+=2;

		}else if (Temp<0x80)
		{
			*pcode = Temp;
			pcode++;
		}
		code+=2;
	}
	*pcode = 0;
}
